generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model driver {
  driver_id  Int      @id
  first_name String   @db.VarChar(255)
  last_name  String   @db.VarChar(255)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
