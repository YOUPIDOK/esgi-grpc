generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model car {
  car_id         Int      @id
  nb_exemplary   Int
  image_filename String   @db.VarChar(255)
  created_at     DateTime @default(now())
  updated_at     DateTime @updatedAt
}
